<?xml version='1.0' encoding='US-ASCII'?>
<sites_list>
    <site>
        <site_name>source</site_name>
        <site_URI>sftp.source_site.edu</site_URI>
        <site_uname>source_user</site_uname>
        <site_password>source_password</site_password>
        <site_remotepath>/smi.xml</site_remotepath>
        <site_contact_email>contact@source_site.edu</site_contact_email>
    </site>
    <site>
        <site_name>destination</site_name>
        <site_URI>sftp.destination_site.edu:50022</site_URI>
        <site_uname>destination_user</site_uname>
        <site_password>destination_password</site_password>
        <site_remotepath>/output.csv</site_remotepath>
        <site_contact_email>contact@destination_site.edu</site_contact_email>
    </site>
    <!--
     SFTP Key-based Authentication

     When using key-based authentication, you should not supply a password
     (omit the "site_password").

     Research Subject Mapper will attempt to use the user's private key. If the
     key is in a non-standard location, you can specify the path using
     "site_key_path" like so:

    <site>
        <site_key_path>/home/user123/.ssh/alternate-id_rsa</site_key_path>
        <site_uname>destination_user</site_uname>
        <site_name>destination</site_name>
        <site_URI>sftp.destination_site.edu:50022</site_URI>
        <site_remotepath>/output.csv</site_remotepath>
        <site_contact_email>contact@destination_site.edu</site_contact_email>
    </site>
    -->
</sites_list>
